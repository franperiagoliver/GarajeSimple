package com.everis.alicante.courses.beca.java_.simple.garage.domain;

import java.util.List;

public class Garaje {

	private List<Plaza> plaza;

	public Garaje(List<Plaza> plazas) {
		plaza = plazas;

	}

	public Plaza getPlaza() {
		return plaza;
	}

	public void setPlaza(Plaza plaza) {
		this.plaza = plaza;
	}

}
